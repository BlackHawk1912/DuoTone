{
  "short_name": "DuoTone",
  "name": "DuoTone",
  "description": "Github Page as a Progressive Web App",
  "scope": "./",
  "start_url": "./",
  "background_color": "#000000",
  "theme_color": "#555555",
  "display": "fullscreen",
  "icons": [
    {
      "src": "img/icon-sm.png",
      "type": "image/png",
      "sizes": "192x192",
      "purpose": "any monochrome maskable"
    },
    {
      "src": "img/icon.png",
      "type": "image/png",
      "sizes": "512x512",
      "purpose": "any monochrome maskable"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "file",
          "accept": ["image/jpeg", ".jpg"]
        }
      ]
    }
  }
}
